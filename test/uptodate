#target: uncached cached cached

target uncached
{
    cache uptodate = getCache("test" 0)
    if !uptodate
    {
        setUptodate(cache)
        cached()
    }
}

target cached
{
    cache uptodate = getCache("test" 0)
    if uptodate
    {
        echo("PASS")
    }
    cache uptodate = getCache("test" 1)
    if uptodate
    {
        echo("FAIL")
    }
}
