test(text, substring, expectedResult)
   i = indexOf(text, substring)
   if i != expectedResult
      echo("FAIL: " text " " substring ": " i)

default:
   test("", "", -1)
   test("abc", "", -1)

   test("abc", "a", 0)
   test("abc", "b", 1)
   test("abc", "c", 2)

   test("abc", "ab", 0)
   test("abc", "bc", 1)
   test("abc", "ac", -1)
   test("abc", "abcd", -1)

   echo("PASS")
